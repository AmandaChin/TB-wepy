<template>
    <!-- <text>{{src}}</text> -->
    <web-view src="{{src}}"></web-view>
</template>
<script>
  import wepy from 'wepy'

  export default class resource extends wepy.page {
    config = {
      navigationBarTitleText: '课程资源'
    }
    data = {
      src:"http://www.baidu.com"
    }
    async onLoad(query) {
      this.src = query.url
      this.$apply()
    }
  }
</script>